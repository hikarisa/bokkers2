<h2>User Info</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true %>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, accept: "image/*" %>  
  <label for="inputName">Introduction</label>
  <%= f.text_field :introduction %>
  <%= f.submit "変更を保存" %>
<% end %>